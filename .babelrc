{
    "presets": [
        "react-native"
    ],
    "plugins": [
        [
            "module-alias",
            [
                {
                    "src": "./app/tabs",
                    "expose": "tabs"
                },
                {
                    "src": "./app",
                    "expose": "app"
                },
                {
                    "src": "./app/data",
                    "expose": "data"
                },
                {
                    "src": "./app/components",
                    "expose": "components"
                },
                {
                    "src": "./app/utils",
                    "expose": "utils"
                }
            ]
        ]
    ]
}